{
  "name": "@metamask/delegation-abis",
  "version": "0.12.0-beta.0",
  "description": "ABIs mapped to versions of the Delegation Framework contracts",
  "license": "(MIT-0 OR Apache-2.0)",
  "type": "module",
  "keywords": [
    "MetaMask",
    "Ethereum"
  ],
  "homepage": "https://github.com/metamask/smart-accounts-kit/tree/main/packages/delegation-abis#readme",
  "bugs": {
    "url": "https://github.com/metamask/smart-accounts-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metamask/smart-accounts-kit.git"
  },
  "author": "MetaMask <hello@metamask.io>",
  "sideEffects": false,
  "contributors": [],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "dist/"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./bytecode": {
      "require": {
        "types": "./dist/bytecode.d.cts",
        "default": "./dist/bytecode.cjs"
      },
      "import": {
        "types": "./dist/bytecode.d.ts",
        "default": "./dist/bytecode.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.18 || >=20"
  },
  "scripts": {
    "build": "yarn typecheck && tsup",
    "typecheck": "tsc --noEmit",
    "changelog:update": "../../scripts/update-changelog.sh @metamask/delegation-abis",
    "changelog:validate": "../../scripts/validate-changelog.sh @metamask/delegation-abis",
    "clean": "rm -rf dist",
    "lint": ""
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^5.0.2",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "5.5.4"
  }
}
